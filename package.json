{
  "name": "farseer-next",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "turbo": "latest"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
